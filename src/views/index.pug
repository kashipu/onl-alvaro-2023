include components/mixins.pug

doctype html
html(lang="es")
    head
        meta(charset="UTF-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        script(src='https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js')
        link(href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" rel="stylesheet")
        link(rel='stylesheet' type='text/css' href='https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css')
        link(rel='stylesheet' type='text/css' href='https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css')
        link(rel="stylesheet" href="css/main.css")
        title Observatorio Nacional de Logística | ONL
    body
        header
            include components/header-gov.pug
            include components/header-logos.pug
            include components/header-nav.pug
        main
            div(class="container")
                div(class="title-h1")
                    h1 Indicadores
                include components/carousel-cards-indicadores.pug
                div(class="grid-anchor-boxes p-2")
                    div(class="title-h1")
                        h2 Otros Indicadores
                    include components/anchor-boxes.pug
                .capsulas-grid
                    div(class="title-h1")
                        h2 Cápsulas
                    .row.py-3.mb-5
                        .col-xl-3.col-md-6.col-sm-12
                            include components/card-capsulas.pug
                        .col-xl-3.col-md-6.col-sm-12
                            include components/card-capsulas.pug
                        .col-xl-3.col-md-6.col-sm-12
                            include components/card-capsulas.pug
                        .col-xl-3.col-md-6.col-sm-12
                            include components/card-capsulas.pug
